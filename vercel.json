{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public"
}
